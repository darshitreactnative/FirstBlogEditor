{
  "expo": {
    "name": "Offline-FirstBlogEditor",
    "slug": "Offline-FirstBlogEditor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "offlinefirstblogeditor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.darshit123.OfflineFirstBlogEditor"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.darshit_123.OfflineFirstBlogEditor",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "versionCode": 1
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "config": {
        "firebase": {
          "apiKey": "AIzaSyC7cnuPnguaB5KhRg43rxblV1WrpVdbvLw",
          "authDomain": "offline-firstblogeditor.firebaseapp.com",
          "databaseURL": "https://offline-firstblogeditor-default-rtdb.firebaseio.com",
          "projectId": "offline-firstblogeditor",
          "storageBucket": "offline-firstblogeditor.firebasestorage.app",
          "messagingSenderId": "450921957584",
          "appId": "1:450921957584:web:d087c7fef511ada34cf6d5"
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "sounds": []
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "98dfb8e4-62d9-4aea-9e71-80f4452d8193"
      },
      "router": {}
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#ffffff"
    }
  }
}
